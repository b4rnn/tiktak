[
    {
        "database": "pb",
        "index": "idxtfdb",
        "setting": {
            "analysis": {
                "analyzer": {
                    "ngram_analyzer": {
                        "filter": [
                            "lowercase"
                        ],
                        "type": "custom",
                        "tokenizer": "ngram_tokenizer"
                    }
                },
                "tokenizer": {
                    "ngram_tokenizer": {
                        "token_chars": [
                            "letter",
                            "digit",
                            "punctuation",
                            "symbol"
                        ],
                        "min_gram": "9",
                        "type": "ngram",
                        "max_gram": "10"
                    }
                }
            }
        },
        "nodes": {
            "table": "trade_f",
            "columns": [
                "date",
                "status",
                "amount",
                "businessid",
		"timestamp"
            ]
        }
    }
]
